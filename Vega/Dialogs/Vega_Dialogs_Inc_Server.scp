ON=100
	if (<Src.Ctag0.VegaServerPage>)
		Src.Ctag0.VegaServerPage -= 1
	endif
	Src.DialogClose d_vega_server
	Src.Dialog d_vega_server
	
ON=101
	if (<Src.Ctag0.VegaServerPage> < <Eval <Def0.VegaServer_Pages>-1>)
		Src.Ctag0.VegaServerPage += 1
	endif
	Src.DialogClose d_vega_server
	Src.Dialog d_vega_server

