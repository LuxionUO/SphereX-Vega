	VegaBtnEmpty 670,470,0,101
endif

[DIALOG d_vega_server BUTTON]
ON=1
	Src.Dialog d_vega_server
	Src.Ctag0.VegaAccPage=
	Src.Ctag0.VegaAccSearch =
	Src.Ctag0.VegaSearch =
	Src.DialogClose d_vega_server_accounts
